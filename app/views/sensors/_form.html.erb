<%= form_for(@sensor) do |f| %>
  <div>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :measurement %><br />
    <%= f.text_field :measurement %>
  </div>
  <div>
    <%= f.label :scale %><br />
    <%= f.text_field :scale %>
  </div>
  <div>
    <%= f.label :value %><br />
    <%= f.text_field :value %>
  </div>
<% end %>
